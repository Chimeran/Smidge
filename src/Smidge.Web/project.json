{
    "webroot": "wwwroot",
    "version": "1.0.0-*",
    "exclude": [
        "wwwroot"
    ],
    "packExclude": [
        "**.kproj",
        "**.user",
        "**.vspscc"
    ],
    "dependencies": {
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta4",
        "Microsoft.AspNet.Server.IIS": "1.0.0-beta4", 
        "Microsoft.AspNet.Mvc": "6.0.0-beta4", 
        "Microsoft.AspNet.Hosting": "1.0.0-beta4",
        "Microsoft.AspNet.Diagnostics": "1.0.0-beta4",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta4",
        "Smidge": "1.0.0-*"
    },
    "commands": {
        "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5009",
        "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5009"
    },
    "frameworks": {
        "dnx451": { } 
        //,"dnxcore50": { }
    }
}
